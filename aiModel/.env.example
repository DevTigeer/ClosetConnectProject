# Google AI API Key (for Gemini Try-On service)
GOOGLE_API_KEY=your_google_api_key_here

# RabbitMQ Configuration (for Worker)
RABBITMQ_HOST=your-railway-rabbitmq-host
RABBITMQ_PORT=5672
RABBITMQ_USER=user
RABBITMQ_PASSWORD=password
RABBITMQ_VHOST=/

# Service Ports (for local development)
# Cloud Run will override these with its own PORT environment variable
SEGMENTATION_PORT=8002
INPAINTING_PORT=8003
TRYON_API_PORT=5001

# Backend URLs (for CORS)
BACKEND_URL=https://your-backend.railway.app
FRONTEND_URL=https://your-frontend.vercel.app
